{
    "name": "cco7ff/endpoint-testing",
    "description": "Accepts access logs, and tries these against a given site to see if they work",
    "type": "composer-plugin",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "potsky/pimp-my-log": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "phploc/phploc": "^3.0",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6",
        "theseer/phpdox": "0.8.1.1",
        "sebastian/phpcpd": "^2.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cory Collier",
            "email": "cory.collier@ahss.org"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"EndpointTesting\\": "src/"}
    }
}
